{
  "expo": {
    "name": "signal-clone",
    "entryPoint": "'../../App.js",
    "slug": "signal-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": [
        "./assets/images/signal-app-icon.png",
        "./assets/images/vecteezy_woman-face-expression-clipart-design-illustration_9397892.png"
      ],
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.signalclone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
